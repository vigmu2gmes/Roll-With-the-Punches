end

function on_message(self, message_id, message, sender)
	if message_id == hash("spawn_enemies") then
		for x = 1, message.spawning do
			factory.create("#enemy_factory", vmath.vector3(math.random(225, 1075), math.random(100, 500), 0))
		end
	end
end
