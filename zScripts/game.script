function init(self)
	factory.create("#face_picker", vmath.vector3(-300, -300, 0))
	
	factory.create("#roll_circle", vmath.vector3(640, 360, 0), nil, {}, .20)
	factory.create("#player_factory", vmath.vector3(300, 360, 0.1), nil, {}, .5)

	math.randomseed(os.time())
end

